<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="com.UI.controllers.ManageServicesDialogPresenter">
    <children>
        <VBox prefHeight="400.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefWidth="200.0">
                    <children>
                        <Label text="Filtruj">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <padding>
                                <Insets left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </Label>
                        <ComboBox fx:id="filterComboBox" prefWidth="150.0">
                            <HBox.margin>
                                <Insets right="10.0"/>
                            </HBox.margin>
                        </ComboBox>
                        <TextField fx:id="filterTextFld"/>
                        <Button fx:id="filterBtn" mnemonicParsing="false" text="Filtruj">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Separator orientation="VERTICAL" prefHeight="30.0" prefWidth="29.0">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Separator>
                        <Button fx:id="newServiceBtn" mnemonicParsing="false" text="Nowa usługa">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="editServiceBtn" mnemonicParsing="false" text="Edytuj usługę">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="deleteServiceBtn" mnemonicParsing="false" text="Usuń wybrane usługi">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" top="5.0"/>
                    </VBox.margin>
                </HBox>
                <TableView fx:id="serviceTableView">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <columns>
                        <TableColumn fx:id="serviceNameCol" prefWidth="648.0000152587891" text="Nazwa"/>
                        <TableColumn fx:id="symbolCol" maxWidth="900.0" minWidth="-1.0" prefWidth="-1.0"
                                     text="    Symbol&#10;PKWIU/PKOB"/>
                        <TableColumn fx:id="unitCol" maxWidth="700.0" minWidth="-1.0" prefWidth="-1.0"
                                     text="Jednostka"/>
                        <TableColumn fx:id="unitPriceCol" maxWidth="1200.0" minWidth="-1.0" prefWidth="-1.0"
                                     text="Cena jednostkowa&#10;       netto (zł)"/>
                        <TableColumn fx:id="vatCol" maxWidth="700.0" minWidth="-1.0" prefWidth="-1.0"
                                     text="Stawka VAT"/>
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
